{
  "name": "seattle-meal-maps-client",
  "version": "0.7.2",
  "description": "A meal finder app using ReactJS",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "babel src --out-dir dist && cp -R src/mapbox-css dist",
    "prepublish": "npm run clean && npm run build",
    "postversion": "git push && git push --tags",
    "app": "webpack-dev-server --config ./webpack.config.dev.js",
    "build-app": "webpack --config ./webpack.config.dev.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seattle-Meal-Maps/Seattle-Meal-Maps-Client"
  },
  "keywords": [
    "mapbox",
    "react",
    "map",
    "webgl",
    "mapbox-gl",
    "react-component"
  ],
  "author": "Lisa Wenke",
  "bugs": {
    "url": "https://github.com/Seattle-Meal-Maps/Seattle-Meal-Maps-Client/issues"
  },
  "homepage": "https://github.com/Seattle-Meal-Maps/Seattle-Meal-Maps-Client#readme",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "mapbox-gl": "^0.20.0",
    "react-mapbox-gl": "^0.7.2",
    "reduce-object": "^0.1.3"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-philpl": "^0.1.0",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "immutable": "^3.8.0",
    "json-loader": "^0.5.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-mixin": "^3.0.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "xml2js": "^0.4.16"
  }
